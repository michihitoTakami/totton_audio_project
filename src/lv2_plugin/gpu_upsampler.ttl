@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://github.com/michihitoTakami/gpu_upsampler>
    a lv2:Plugin ;
    doap:name "GPU Audio Upsampler" ;
    doap:description "Ultra-high precision audio upsampling using GPU (1M tap minimum phase FIR, 44.1kHz->705.6kHz)" ;
    doap:license <https://opensource.org/licenses/MIT> ;
    doap:maintainer [
        foaf:name "Michihito Takami" ;
        foaf:homepage <https://github.com/michihitoTakami>
    ] ;
    lv2:project <http://github.com/michihitoTakami/gpu_upsampler> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "Input Left" ;
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in_r" ;
        lv2:name "Input Right" ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 2 ;
        lv2:symbol "out_l" ;
        lv2:name "Output Left" ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 3 ;
        lv2:symbol "out_r" ;
        lv2:name "Output Right" ;
    ] .
